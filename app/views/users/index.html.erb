<%= provide :title, t(".title") %>
<h1><%= t ".title" %></h1>
<div class="container">
  <div class="row">
    <div class="col-md-6 col-xs-12 col-md-offset-3">
      <%= form_tag users_path, method: :get, class: "form_search_user" do %>
        <%= render "search_form" %>
      <% end %>
      <div class="table-responsive">
        <table class="table table-hover" id="index-user-member">
          <%= render @users %>
        </table>
        <center><%= will_paginate %></center>
      </div>
    </div>
  </div>
</div>
